#!/usr/bin/make -f

DEB_DH_INSTALL_SOURCEDIR = debian/tmp
INSTALL_DIR = $(CURDIR)/$(DEB_DH_INSTALL_SOURCEDIR)
PACKAGE_NAME = onlp



%:
	dh $@

build-arch:
	dh build-arch

clean:
	dh clean

SO := libonlp.so

override_dh_shlibdeps:
	@echo "Skipping dh_shlibdeps"

override_dh_auto_install:
	@echo "Fill me out."
	mkdir -p $(INSTALL_DIR)/lib/$(DEB_HOST_GNU_TYPE)
	cp ../../build/powerpc-linux-gnu/bin/$(SO) $(INSTALL_DIR)/lib/$(DEB_HOST_GNU_TYPE)/




